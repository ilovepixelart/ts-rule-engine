{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "eslint.options": {
    "extensions": [
      ".js",
      ".ts"
    ]
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "cSpell.words": [
    "clonedeep",
    "Dsonar",
    "ilovepixelart",
    "isequal",
    "lcov",
    "linebreak",
    "nargs",
    "nodenext",
    "npmignore",
    "parens",
    "rhysd",
    "singleline",
    "sonarcloud",
    "sonarjs",
    "sonarsource",
    "swcrc"
  ],
  "sonarlint.connectedMode.project": {
    "connectionId": "ilovepixelart",
    "projectKey": "ilovepixelart_ts-rule-engine"
  }
}